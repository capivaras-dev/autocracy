generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String? @db.VarChar(255)
  email String  @unique @db.VarChar(255)
}

model Pastebin {
  id        String   @id
  editCode  String
  lifetime  BigInt
  createdAt DateTime @default(now())
}
